{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": false,
    "quotemark": [true, "single"],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "trailing-comma": false,
    "ordered-imports": false,
    "whitespace": [
      false,
      "check-branch"
    ]
  },
  "exclude": [
    "./node_modules"
  ]
}